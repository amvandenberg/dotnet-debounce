<Project>

  <PropertyGroup>
    <!-- Use the latest .NET Core SDK -->
    <TargetFramework>net5.0</TargetFramework>
    <!-- Use the latest C# Language standard -->
    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
    <AnalysisMode>AllEnabledByDefault</AnalysisMode>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    
    <!-- Common defaults -->
    <IsPackable>false</IsPackable>
    <Authors>Frans van Dorsselaer</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright (c) 2020 Frans van Dorsselaer</Copyright>
    <PackageProjectUrl>https://github.com/dorssel/TestRepo</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dorssel/TestRepo.git</RepositoryUrl>
  </PropertyGroup>

</Project>

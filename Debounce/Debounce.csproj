<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.2;netstandard2.0</TargetFrameworks>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <RootNamespace>Dorssel.Utility</RootNamespace>
    <AssemblyName>Dorssel.Utility.Debounce</AssemblyName>
    <Title>Event Debouncer</Title>
    <Authors>Frans van Dorsselaer</Authors>
    <Description>Debouncer for events 2.</Description>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.1.0</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright (c) 2020 Frans van Dorsselaer</Copyright>
    <PackageProjectUrl>https://github.com/dorssel/TestRepo</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dorssel/TestRepo.git</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.8">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
